{
    "routes": [{ "src": "/(.*)", "dest": "/" }],
    "rewrites": [
        {
          "source": "/api/:path*",
          "destination": "https://astronomy-app-api.vercel.app/:path*"
        },
        {
          "source": "/api/:path*/",
          "destination": "https://astronomy-app-api.vercel.app/:path*/"
        }
    ]
}