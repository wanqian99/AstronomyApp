{
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
    "rewrites": [
        {
          "source": "/api/:path*",
          "destination": "https://backend-endpoint/:path*"
        },
        {
          "source": "/api/:path*/",
          "destination": "https://backend-endpoint/:path*/"
        }
    ]
}